// config.h.in platfrom plugin output
// set(DFM_PLUGIN_PATH) in cmake
// if setting new definition, please use:
//  #undef DFM_PLUGIN_PATH
//  #define DFM_PLUGIN_PATH xxx

#ifndef RUNTIME_INSTALL_PATH
#define RUNTIME_INSTALL_PATH "${RUNTIME_INSTALL_RPEFIX}"
#else
    #error "cmake unseting definition RUNTIME_INSTALL_RPEFIX"
#endif

#ifndef RUNTIME_BUILD_PATH
#define RUNTIME_BUILD_PATH "${CMAKE_RUNTIME_OUTPUT_DIRECTORY}"
#else
    #error "cmake unseting definition CMAKE_RUNTIME_OUTPUT_DIRECTORY"
#endif

#ifndef PLUGIN_INSTALL_PATH
#define PLUGIN_INSTALL_PATH "${PLUGIN_INSTALL_PATH}"
#elif
    #error "cmake unseting definition PLUGIN_INSTALL_PATH"
#endif

#ifndef PLUGIN_BUILD_PATH
#define PLUGIN_BUILD_PATH "${CMAKE_LIBRARY_OUTPUT_DIRECTORY}"
#elif
    #error "cmake unseting definition CMAKE_LIBRARY_OUTPUT_DIRECTORY"
#endif
